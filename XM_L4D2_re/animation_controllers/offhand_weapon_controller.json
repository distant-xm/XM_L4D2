{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.l4d2.deagle_controller": {
      "initial_state": "holding",
      "states": {
        "holding": {
          "animations": [
            "deagle_holding"
          ],
          "transitions": [
            {
              "reloading": "q.property('l4d2:reloading') == 1"
            },
            {
              "shoot": "q.property('l4d2:o_ammo') > 0 && q.is_using_item"
            },
            {
              "pushing": "q.mod.l4d2_attack == 5"
            }
          ]
        },
        "reloading": {
          "animations": [
            "deagle_reloading"
          ],
          "transitions": [
            {
              "holding": "q.property('l4d2:reloading') == 0"
            }
          ]
        },
        "shoot": {
          "animations": [
            "deagle_shoot"
          ],
          "transitions": [
            {
              "holding": "q.any_animation_finished && !q.is_using_item"
            }
          ]
        },
        "pushing": {
          "animations": [
            "deagle_pushing"
          ],
          "transitions": [
            {
              "holding": "q.all_animations_finished"
            }
          ]
        }
      }
    },
    "controller.animation.l4d2.two_pistol_controller": {
      "initial_state": "holding",
      "states": {
        "holding": {
          "animations": [
            "two_pistol_holding"
          ],
          "transitions": [
            {
              "reloading": "q.property('l4d2:reloading') == 1"
            },
            {
              "shoot_right": "q.property('l4d2:o_ammo') > 0 && q.is_using_item && v.two_pistol_shoot == 1"
            },
            {
              "shoot_left": "q.property('l4d2:o_ammo') > 0 && q.is_using_item && v.two_pistol_shoot == 2"
            },
            {
              "pushing": "q.mod.l4d2_attack == 5"
            }
          ]
        },
        "reloading": {
          "animations": [
            "two_pistol_reloading"
          ],
          "transitions": [
            {
              "holding": "q.property('l4d2:reloading') == 0"
            }
          ]
        },
        "shoot_right": {
          "animations": [
            "two_pistol_shoot_right"
          ],
          "on_exit": [
            "v.two_pistol_shoot = 2;"
          ],
          "transitions": [
            {
              "holding": "q.any_animation_finished && !q.is_using_item"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "shoot_left": {
          "animations": [
            "two_pistol_shoot_left"
          ],
          "on_exit": [
            "v.two_pistol_shoot = 1;"
          ],
          "transitions": [
            {
              "holding": "q.any_animation_finished && !q.is_using_item"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "pushing": {
          "animations": [
            "two_pistol_pushing"
          ],
          "transitions": [
            {
              "holding": "q.all_animations_finished"
            }
          ]
        }
      }
    },
