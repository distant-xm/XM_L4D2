    "controller.animation.l4d2.throws_controller": {
      "initial_state": "holding",
      "states": {
        "holding": {
          "animations": [
            "throw_holding"
          ],
          "transitions": [
            {
              "throws_1": "q.property('l4d2:throws_using') == 1"
            },
            {
              "pushing": "q.property('l4d2:push') == 1"
            }
          ]
        },
        "throws_1": {
          "animations": [
            "throw_throwing_1"
          ],
          "transitions": [
            {
              "throws_2": "q.property('l4d2:throws_using') == 2"
            },
            {
              "holding": "q.property('l4d2:throws_using') == 0"
            }
          ],
          "blend_transition": 0.2
        },
        "throws_2": {
          "animations": [
            "throw_throwing_2",
            { "bill_molotov": "q.property('l4d2:player_id') == 1" },
            { "zoey_molotov": "q.property('l4d2:player_id') == 2" },
            { "louis_molotov": "q.property('l4d2:player_id') == 3" },
            { "francis_molotov": "q.property('l4d2:player_id') == 4" }
          ],
          "transitions": [
            {
              "holding": "q.property('l4d2:throws_using') == 0"
            }
          ],
          "blend_transition": 0.2
        },
        "pushing": {
          "animations": [
            "throw_pushing"
          ],
          "transitions": [
            {
              "holding": "q.all_animations_finished"
            }
          ]
        }
      }
