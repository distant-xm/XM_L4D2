{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.l4d2.weapon_controller": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "check": "(1.0)"
            }
          ]
        },
        "check": {
          "transitions": [
            {
              "shovel": "q.mod.l4d2_item == 15"
            },
            {
              "riot_shield": "q.mod.l4d2_item == 14"
            },
            {
              "pitchfork": "q.mod.l4d2_item == 13"
            },
            {
              "nightstick": "q.mod.l4d2_item == 12"
            },
            {
              "machete": "q.mod.l4d2_item == 11"
            },
            {
              "knife": "q.mod.l4d2_item == 10"
            },
            {
              "katana": "q.mod.l4d2_item == 9"
            },
            {
              "golf_club": "q.mod.l4d2_item == 8"
            },
            {
              "frying_pan": "q.mod.l4d2_item == 7"
            },
            {
              "fireaxe": "q.mod.l4d2_item == 6"
            },
            {
              "electric_guitar": "q.mod.l4d2_item == 5"
            },
            {
              "crowbar": "q.mod.l4d2_item == 4"
            },
            {
              "cricket_bat": "q.mod.l4d2_item == 3"
            },
            {
              "chainsaw": "q.mod.l4d2_item == 2"
            },
            {
              "baseball_bat": "q.mod.l4d2_item == 1"
            }
          ]
        },
        "shovel": {
          "animations": [
            "shovel_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 15"
            }
          ]
        },
        "riot_shield": {
          "animations": [
            "riot_shield_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 14"
            }
          ]
        },
        "pitchfork": {
          "animations": [
            "pitchfork_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 13"
            }
          ]
        },
        "nightstick": {
          "animations": [
            "nightstick_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 12"
            }
          ]
        },
        "machete": {
          "animations": [
            "machete_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 11"
            }
          ]
        },
        "knife": {
          "animations": [
            "knife_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 10"
            }
          ]
        },
        "katana": {
          "animations": [
            "katana_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 9"
            }
          ]
        },
        "golf_club": {
          "animations": [
            "golf_club_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 8"
            }
          ]
        },
        "frying_pan": {
          "animations": [
            "frying_pan_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 7"
            }
          ]
        },
        "fireaxe": {
          "animations": [
            "fireaxe_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 6"
            }
          ]
        },
        "electric_guitar": {
          "animations": [
            "electric_guitar_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 5"
            }
          ]
        },
        "crowbar": {
          "animations": [
            "crowbar_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 4"
            }
          ]
        },
        "cricket_bat": {
          "animations": [
            "cricket_bat_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 3"
            }
          ]
        },
        "chainsaw": {
          "animations": [
            "chainsaw_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 2"
            }
          ]
        },
        "baseball_bat": {
          "animations": [
            "baseball_bat_controller"
          ],
          "transitions": [
            {
              "check": "!q.mod.l4d2_item == 1"
            }
          ]
        }
      }
    },
    "controller.animation.l4d2.throws_controller": {
      "initial_state": "holding",
      "states": {
        "holding": {
          "animations": [
            "throw_holding"
          ],
          "transitions": [
            {
              "throws_1": "q.property('l4d2:throws_using') == 1"
            },
            {
              "pushing": "q.property('l4d2:push') == 1"
            }
          ]
        },
        "throws_1": {
          "animations": [
            "throw_throwing_1"
          ],
          "transitions": [
            {
              "throws_2": "q.property('l4d2:throws_using') == 2"
            },
            {
              "holding": "q.property('l4d2:throws_using') == 0"
            }
          ],
          "blend_transition": 0.2
        },
        "throws_2": {
          "animations": [
            "throw_throwing_2",
            { "bill_molotov": "q.property('l4d2:player_id') == 1" },
            { "zoey_molotov": "q.property('l4d2:player_id') == 2" },
            { "louis_molotov": "q.property('l4d2:player_id') == 3" },
            { "francis_molotov": "q.property('l4d2:player_id') == 4" }
          ],
          "transitions": [
            {
              "holding": "q.property('l4d2:throws_using') == 0"
            }
          ],
          "blend_transition": 0.2
        },
        "pushing": {
          "animations": [
            "throw_pushing"
          ],
          "transitions": [
            {
              "holding": "q.all_animations_finished"
            }
          ]
        }
      }
    }
  }
}