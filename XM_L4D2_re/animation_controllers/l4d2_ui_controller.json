{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.l4d2_ui": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"ui_position_controller",
						"ui_ammo_controller",
						"ui_mainhand_weapon_controller",
						"ui_offhand_weapon_controller",
						"ui_throws_controller",
						"ui_firstaidkit_controller",
						"ui_medicines_controller"
					]
				}
			}
		},
		"controller.animation.l4d2.ui_temporary_hp": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"temporary_hp_check": "v.temporary_hp = (q.health / q.max_health); return q.property('l4d2:temporary_hp_has');"
						}
					]
				},
				"temporary_hp_check": {
					"transitions": [
						{
							"temporary_hp_not": "!q.property('l4d2:temporary_hp_has')"
						}
					]
				},
				"temporary_hp_not": {
					"transitions": [
						{
							"default": "(1.0)"
						}
					]
				}
			}
		},
		"controller.animation.l4d2.ui_position_controller": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"ui_position",
						{
							"ui_position_ride": "q.is_riding"
						},
						{
							"ui_position_swim": "q.is_swimming || q.is_crawling"
						},
						{
							"ui_position_glide": "q.is_gliding"
						}
					]
				}
			}
		},



		
		"controller.animation.l4d2.ui_ammo_controller": {
			"initial_state": "default",
			"states": {
				"default": {
					"on_entry": [
						"v.ui_ammo = 0;"
					],
					"transitions": [
						{
							"check": "(1.0)"
						}
					]
				},
				"check": {
					"on_entry": [
						"v.ui_ammo = 0;"
					],
					"transitions": [
						{
							"rifle": "q.is_item_name_any('slot.hotbar', 0, 'l4d2:ak47', 'l4d2:awp')"
						},
						{
							"shot": "q.is_item_name_any('slot.hotbar', 0, 'l4d2:shotgun_spas')"
						},
						{
							"grenade": "q.is_item_name_any('slot.hotbar', 0, 'l4d2:grenade_launcher')"
						}
					]
				},
				"rifle": {
					"on_entry": [
						"v.ui_ammo = 1;"
					],
					"transitions": [
						{
							"check": "!q.is_item_name_any('slot.hotbar', 0, 'l4d2:ak47')"
						}
					]
				},
				"shot": {
					"on_entry": [
						"v.ui_ammo = 2;"
					],
					"transitions": [
						{
							"check": "!q.is_item_name_any('slot.hotbar', 0, 'l4d2:shotgun_spas')"
						}
					]
				},
				"grenade": {
					"on_entry": [
						"v.ui_ammo = 3;"
					],
					"transitions": [
						{
							"check": "!q.is_item_name_any('slot.hotbar', 0, 'l4d2:grenade_launcher')"
